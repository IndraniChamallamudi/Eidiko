

CREATE COMPUTE MODULE file_msg_flow_Compute
	DECLARE I SHARED INTEGER 1;
	
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		-- CALL CopyMessageHeaders();
		CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		SET I = I + 1;
		
		SET OutputLocalEnvironment.Destination.File.NAME = CAST(I AS CHARACTER)||'.txt';
		--SSET OutputLocalEnvironment.Destination.V.D='C:\Users\Administrator\Desktop\output';
	END;
END MODULE;
